    "import random as rnd\n",
    "\n",
    "def aprox(n):\n",
    "    k = 0\n",
    "\n",
    "    for _ in range(n):\n",
    "        x = rnd.uniform(-1, 1)\n",
    "        y = rnd.uniform(-1, 1)\n",
    "        if(x**2 + y**2 <= 1):\n",
    "            k +=1\n",
    "\n",
    "\n",
    "    return 4 * (k/n)\n",
    "       \n",
    "aprox(100000)\n"
